{
  "require": {
    "born05/craft-enforcepassword": "^1.0",
    "codemonauts/craft-readonly-field": "^1.0",
    "craftcms/cms": "^3.4",
    "craftcms/feed-me": "^4.2",
    "craftcms/redactor": "^2.6",
    "doublesecretagency/craft-digitaldownload": "^2.1",
    "doublesecretagency/craft-upvote": "^2.1",
    "rias/craft-scout": "^2.2",
    "solspace/craft-freeform": "^3.6",
    "thejoshsmith/craft-fab-permissions": "^1.3",
    "trendyminds/algolia": "^2.0",
    "twentyfourhoursmedia/views-work": "^1.1",
    "verbb/workflow": "1.4.2",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.3"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
