{
  "require": {
    "barrelstrength/sprout-email": "^4.0",
    "craftcms/cms": "^3.4.0-RC3",
    "craftcms/feed-me": "4.2.0.1",
    "craftcms/redactor": "2.3.3.2",
    "doublesecretagency/craft-digitaldownload": "^2.1",
    "doublesecretagency/craft-upvote": "^2.0",
    "ether/tags": "^1.0",
    "intoeetive/protectedlinks": "^0.0.3",
    "rias/craft-scout": "^2.1",
    "solspace/craft-freeform": "^3.2",
    "studioespresso/craft-seeder": "^3.2",
    "trendyminds/algolia": "^2.0",
    "verbb/workflow": "1.2.2.1",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
